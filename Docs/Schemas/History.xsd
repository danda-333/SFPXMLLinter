<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           elementFormDefault="qualified">

  <!-- SFP.Kernel.Model.WorkFlow.Actions.HistoryAction -->
  <xs:element name="History">
    <xs:complexType>

      <!-- Attributes -->
      <xs:attribute name="Text" type="xs:string" use="optional" />
      <xs:attribute name="TextResourceKey" type="xs:string" use="optional" />
      <xs:attribute name="HistoryType" type="xs:string" use="optional" />
      <xs:attribute name="ActionStart" type="xs:string" use="optional" />
      <xs:attribute name="RunPriority" type="xs:int" use="optional" default="1" />
      <xs:attribute name="Ident" type="xs:string" use="optional" />
    </xs:complexType>
  </xs:element>


</xs:schema>
