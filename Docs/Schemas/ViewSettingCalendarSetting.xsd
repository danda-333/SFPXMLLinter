      <xs:attribute name="IsOmitZeroMinute" type="xs:boolean" use="optional" default="false" />
      <xs:attribute name="Second" type="xs:string" use="optional" default="" />
          </xs:complexType>
        </xs:element>
      </xs:all>

      <!-- Attributes -->
      <xs:attribute name="ViewType" type="xs:string" use="optional" default="None" />
      <xs:attribute name="Name" type="xs:string" use="optional" />
      <xs:attribute name="Type" type="xs:string" use="optional" default="None" />
      <xs:attribute name="ButtonText" type="xs:string" use="optional" default="" />
