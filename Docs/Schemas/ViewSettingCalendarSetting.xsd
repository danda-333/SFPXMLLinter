          </xs:complexType>
        </xs:element>
        <xs:element name="MultiMonth" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:all>
              <xs:element name="TitleFormat" type="xs:anyType" minOccurs="0" maxOccurs="1" />
            </xs:all>

            <!-- Attributes -->
      <xs:attribute name="MaxColumn" type="xs:int" use="optional" default="0" />
      <xs:attribute name="MinWidth" type="xs:int" use="optional" default="0" />
          </xs:complexType>
        </xs:element>
        <xs:element name="Duration" minOccurs="0" maxOccurs="1">
          <xs:complexType>

            <!-- Attributes -->
      <xs:attribute name="Day" type="xs:int" use="optional" default="0" />
      <xs:attribute name="Week" type="xs:int" use="optional" default="0" />
      <xs:attribute name="Month" type="xs:int" use="optional" default="0" />
