              <dsf:string>ITSMOperationalEnvironmentTypeGuid</dsf:string>
            </dsf:Columns>
            <dsf:SQL>
              LEFT JOIN usr.ITSMOperationalEnvironmentType as opeEnvTyp on opeEnvTyp.Ident = itsReq.ITSMOperationalEnvironmentTypeGuid AND opeEnvTyp.LanguageID = @UserLanguageID
            </dsf:SQL>
          </dsf:From>
          <dsf:From Ident="chaClaTyp">
            <dsf:Columns>
              <dsf:string>ITSMChangeClassificationTypeGuid</dsf:string>
            </dsf:Columns>
            <dsf:SQL>
              LEFT JOIN usr.ITSMChangeClassificationType as chaClaTyp on chaClaTyp.Ident = itsReq.ITSMChangeClassificationTypeGuid AND chaClaTyp.LanguageID = @UserLanguageID
            </dsf:SQL>
          </dsf:From>
        </Froms>
        <SQL>

          SELECT 
            itsReq.ID
            #ADDCOLUMN#
          FROM usr.ITSMChange AS itsReq
          INNER JOIN usr.ITSM_FolderTreeView AS ftvCom ON ftvCom.FolderTreeIdent = itsReq.ITSMCompanyIdent
          INNER JOIN dbo.Account AS accCreator ON accCreator.ID = itsReq.AccountID
          #ADDFROM#
          WHERE
            itsReq.[State] != @DeletedState
            AND (
              (
                ftvCom.IsShareAmongAuthors = 0 AND #PERMISSION[ITSMChange(itsReq)|ITSMChangeReporter|]#
              )
              OR
              (
                ftvCom.IsShareAmongAuthors = 1 AND #PERMISSION[ITSMChange(itsReq)|ITSMChangeReporter,ITSMAuthor|]#
              )
              OR
              (
                #PERMISSION[ITSMChange(itsReq)]#
              )
            ) #FILTER#

        </SQL>
        <Parameters>
          <dsp:Parameter xsi:type="dsp:ValueParameter" Ident="DeletedState" DataType="Number" Value="0" />
          <dsp:Parameter xsi:type="dsp:VariableParameter" Ident="UserLanguageID" ConstantType="UserLanguageID" DataType="Number"/>
        </Parameters>
      </DataSource>
    </Section>
  </Sections>

</DataView>