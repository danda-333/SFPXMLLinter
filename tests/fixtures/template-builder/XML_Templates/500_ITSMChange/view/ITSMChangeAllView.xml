        AND (
          (
            ftvCom.IsShareAmongAuthors = 0 AND #PERMISSION[ITSMChange(itsReq)|ITSMChangeReporter|]#
          )
          OR
          (
            ftvCom.IsShareAmongAuthors = 1 AND #PERMISSION[ITSMChange(itsReq)|ITSMChangeReporter,ITSMAuthor|]#
          )
          OR
          (
            #PERMISSION[ITSMChange(itsReq)]#
          )
        ) #FILTER#
    </SQL>
